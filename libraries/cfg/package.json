{
  "name": "cfg",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -b ./ts/tsconfig.config.json",
    "generate": "rnuc --project-root ../../../../apps/client --lib-root ./node_modules/react-native-ultimate-config config.yaml",
    "postinstall": "pnpm run generate && pnpm run build"
  },
  "exports": {
    ".": "./generated/rnuc.js",
    "./babel.config": "./generated/babel.js",
    "./lingui.config": "./generated/lingui.js",
    "./metro.config": "./generated/metro.js",
    "./vite.config": "./generated/vite.js",
    "./vocs.config": "./generated/vocs.js",
    "./ts/tsconfig.app.json": "./ts/tsconfig.app.json",
    "./ts/tsconfig.base.json": "./ts/tsconfig.base.json",
    "./ts/tsconfig.config.json": "./ts/tsconfig.config.json",
    "./ts/tsconfig.native.json": "./ts/tsconfig.native.json",
    "./ts/tsconfig.web.json": "./ts/tsconfig.web.json"
  },
  "dependencies": {
    "@lingui/cli": "^4.7.2",
    "@lingui/conf": "^4.7.2",
    "@lingui/vite-plugin": "^4.7.2",
    "@react-native/metro-config": "0.75.0-main",
    "@rnx-kit/metro-config": "^1.3.15",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.35",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-macros": "^3.1.0",
    "million": "^3.0.6",
    "react-native": "0.73.6",
    "react-native-ultimate-config": "^6.0.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vocs": "latest"
  }
}
