{
  "name": "cfg",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc --project ./ts/tsconfig.config.json",
    "generate": "rnuc --project-root ../../../../apps/client --lib-root ./node_modules/react-native-ultimate-config config.yaml",
    "postinstall": "pnpm run generate && pnpm run build"
  },
  "exports": {
    ".": "./gen/index.js",
    "./babel": "./gen/babel.js",
    "./lingui": "./gen/lingui.js",
    "./metro": "./gen/metro.js",
    "./vite": "./gen/vite.js",
    "./vocs": "./gen/vocs.js",
    "./ts/tsconfig.app.json": "./ts/tsconfig.app.json",
    "./ts/tsconfig.config.json": "./ts/tsconfig.config.json"
  },
  "dependencies": {
    "@lingui/cli": "^4.8.0",
    "@lingui/conf": "^4.8.0",
    "@lingui/vite-plugin": "^4.8.0",
    "@react-native/metro-config": "0.75.0-main",
    "@rnx-kit/metro-config": "^1.3.15",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.35",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-macros": "^3.1.0",
    "million": "^3.0.6",
    "react-native": "0.73.6",
    "react-native-ultimate-config": "^6.0.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vocs": "latest",
    "yaml": "latest"
  }
}
