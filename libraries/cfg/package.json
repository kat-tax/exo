{
  "name": "cfg",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm run gen-config && tsc --project ./types/tsconfig.cfg.json && pnpm run gen-files",
    "gen-config": "rnuc --project-root ../../../../apps/client --lib-root ./node_modules/react-native-ultimate-config config.yaml",
    "gen-files": "tsx ./scripts/generate.ts",
    "postinstall": "pnpm run build"
  },
  "bin": {
    "exo": "./node_modules/vite/bin/vite.js"
  },
  "exports": {
    ".": "./node_modules/react-native-ultimate-config",
    "./app.json": "./gen/app.json",
    "./docs": "./gen/docs.js",
    "./locales": "./gen/locales.js",
    "./babel": "./gen/tools/native/babel.js",
    "./metro": "./gen/tools/native/metro.js",
    "./vite.base": "./gen/tools/web/vite.base.js",
    "./vite.client": "./gen/tools/web/vite.client.js",
    "./vite.lib": "./gen/tools/web/vite.lib.js",
    "./types/tsconfig.app.json": "./types/tsconfig.app.json",
    "./types/tsconfig.cfg.json": "./types/tsconfig.cfg.json"
  },
  "dependencies": {
    "@lingui/core": "^4.8.0",
    "react-native-ultimate-config": "^6.0.1"
  },
  "devDependencies": {
    "@lingui/vite-plugin": "^4.8.0",
    "@react-native/metro-config": "0.75.0-main",
    "@rnx-kit/metro-config": "^1.3.15",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.35",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-macros": "^3.1.0",
    "lightningcss": "^1.24.1",
    "million": "^3.0.6",
    "react-native": "0.73.6",
    "tsx": "latest",
    "vite": "^5.2.7",
    "vite-plugin-dts": "^3.7.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vocs": "latest",
    "yaml": "latest"
  }
}
