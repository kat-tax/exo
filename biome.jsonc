{
  "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "suspicious": {
        "noShadowRestrictedNames": "off"
      }
    }
  },
  "overrides": [{
    "include": ["client/src/**"],
    "linter": {
      "rules": {
        "complexity": {
          "useLiteralKeys": "off"
        }
      }
    }
  }]
}
