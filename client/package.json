{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "conc -c 'auto' 'pnpm:start:*'",
    "ios": "react-native run-ios --no-packager",
    "android": "react-native run-android --no-packager",
    "build": "conc -c 'auto' -g 'pnpm:build:*'",
    "build:web": "vite build",
    "build:ios": "react-native build-ios",
    "build:android": "react-native build-android",
    "start:web": "vite dev --port 8181 --host",
    "start:native": "react-native start --reset-cache",
    "generate:pods": "pod install --project-directory=ios",
    "generate:icons": "react-native-svg-app-icon generate --background-path ../build/config/gen/logo-bg.svg --foreground-path ../build/config/gen/logo-padded.svg"
  },
