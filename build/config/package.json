{
  "name": "config",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "vite": "./node_modules/vite/bin/vite.js"
  },
  "scripts": {
    "build": "pnpm gen:config && pnpm gen:locales && pnpm gen:slices && pnpm gen:logos && tsc",
    "gen:config": "rnuc --project-root ../../client --lib-root ./node_modules/react-native-ultimate-config ../../config.yaml",
    "gen:locales": "node ./scripts/gen-locales.js",
    "gen:logos": "node ./scripts/gen-logos.js",
    "gen:slices": "node ./scripts/gen-slices.js"
  },
  "exports": {
    ".": {
      "types": "./gen/index.d.ts",
      "import": "./gen/index.js",
      "require": "./gen/index.cjs"
    },
    "./locales": "./gen/locales.js",
    "./lingui": "./gen/lingui.js",
    "./vite": "./gen/vite.config.js",
    "./babel": "./gen/babel.config.js",
    "./metro": "./metro.config.cjs",
    "./tsconfig.app.json": "./tsconfig.app.json"
  },
  "dependencies": {
    "react-native-ultimate-config": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@lingui/babel-plugin-lingui-macro": "^5.5.0",
    "@lingui/conf": "^5.5.0",
    "@lingui/core": "^5.5.0",
    "@lingui/vite-plugin": "^5.5.0",
    "@react-native/babel-preset": "0.80.2",
    "@rnx-kit/metro-config": "^2.1.1",
    "@rnx-kit/metro-resolver-symlinks": "^0.2.6",
    "@types/babel__core": "^7.20.5",
    "@vite-pwa/assets-generator": "^1.0.1",
    "@vitejs/plugin-react": "4.7.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "lightningcss": "^1.30.1",
    "react-exo": "0.20.15",
    "react-native-reanimated": "^3.19.1",
    "react-native-unistyles": "^3.0.12",
    "sharp": "^0.34.3",
    "sharp-ico": "^0.1.5",
    "sonda": "latest",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
