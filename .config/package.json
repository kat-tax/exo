{
  "name": "config",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && pnpm run gen-cfg && pnpm run gen-app-json",
    "gen-cfg": "rnuc --project-root ../apps/client --lib-root ./node_modules/react-native-ultimate-config config.client.yaml",
    "gen-app-json": "node ./scripts/gen-app-json.cjs"
  },
  "exports": {
    ".": "./gen/src/index.js",
    "./app.json": "./gen/app.json",
    "./locales": "./gen/config.locales.js",
    "./docs": "./gen/config.docs.js",
    "./storybook": "./gen/config.storybook.js",
    "./tsconfig.app.json": "./ts/app.tsconfig.json",
    "./tsconfig.node.json": "./ts/node.tsconfig.json"
  },
  "dependencies": {
    "react-native-ultimate-config": "latest"
  },
  "devDependencies": {
    "vocs": "latest"
  }
}
